<% if @team.errors.any? %>
    <ol>
        <% @team.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
        <% end %>
    </ol>
<% end %>
<%= form_for @team do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
    <%= f.label :pokemon_id %>
    <%= f.collection_check_boxes :pokemon_ids, @current_user.pokemon.all, :id, :name  %>
    <%= f.submit %>
<% end %>